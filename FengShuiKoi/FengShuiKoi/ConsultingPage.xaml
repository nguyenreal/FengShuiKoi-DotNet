<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FengShuiKoi"
        xmlns:Wpf="clr-namespace:Microsoft.Web.WebView2.Wpf;assembly=Microsoft.Web.WebView2.Wpf" x:Class="FengShuiKoi.ConsultingPage"
        mc:Ignorable="d"
        Title="ConsultingPage" Height="450" Width="800"
        WindowStartupLocation="CenterScreen">
    <Grid>
        <DatePicker x:Name="dpBirthdate" HorizontalAlignment="Center" Height="36" Margin="0,227,0,0" VerticalAlignment="Top" Width="474"/>
        <Label Content="Choose your birthday: " HorizontalAlignment="Center" Height="39" Margin="0,155,0,0" VerticalAlignment="Top" Width="132"/>
        <Button x:Name="btnConsult" Content="Consult" HorizontalAlignment="Center" Height="43" Margin="0,296,0,0" VerticalAlignment="Top" Width="166" Click="btnConsult_Click"/>
        <Button x:Name="btnReturn" Content="Return to Homepage" HorizontalAlignment="Left" Height="38" Margin="18,45,0,0" VerticalAlignment="Top" Width="155" Click="btnReturn_Click"/>
    </Grid>
</Window>
