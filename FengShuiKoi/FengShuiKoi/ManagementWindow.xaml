<Window x:Class="FengShuiKoi.ManagementWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FengShuiKoi"
        mc:Ignorable="d"
        Title="ManagementWindow" Height="650" Width="1000" WindowStartupLocation="CenterScreen">
    <Grid>
        <Button x:Name="btnReturn" Content="Return to Homepage" HorizontalAlignment="Left" Height="38" Margin="49,45,0,0" VerticalAlignment="Top" Width="155" Click="btnReturn_Click"/>
        <Label Content="Management" HorizontalAlignment="Center" Margin="0,111,0,0" VerticalAlignment="Top" FontSize="70"/>
        <Button x:Name="btnKoiFish" Content="Koi fishes" HorizontalAlignment="Left" Margin="342,293,0,0" VerticalAlignment="Top" Height="106" Width="309" FontSize="35" Click="btnKoiFish_Click"/>
        <Button x:Name="btnTank" Content="Tanks" HorizontalAlignment="Center" Margin="0,449,0,0" VerticalAlignment="Top" Height="106" Width="308" FontSize="35" Click="btnTank_Click"/>
    </Grid>
</Window>
